extends ../layout
block content
	.container
		.row
			table.table.table-hover.table-bordered
				thead
					tr
						th 名字
						th 时间
						//- th 查看
						//- th 修改
						//- th 删除
					tbody
						each item in users
							tr(class="item-id-#{item._id}")
								td #{item.name}
								td #{item.meta.createdAt}
								